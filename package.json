{
  "name": "testing-project-week",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./bin/www.js",
    "dev": "nodemon -r dotenv/config ./bin/www.js",
    "db:dropAllTables": "cross-env NODE_ENV=test node -r dotenv/config ./db/scripts/users/dropUsersTable.js",
    "db:createAllTables": "cross-env NODE_ENV=test node -r dotenv/config ./db/scripts/users/createUsersTable.js",
    "db:populateAllTables": "cross-env NODE_ENV=test node -r dotenv/config ./db/scripts/users/populateUsersTable.js",
    "000pretest": "cross-env NODE_ENV=test npm run db:dropAllTables && cross-env NODE_ENV=test npm run db:createAllTables && cross-env NODE_ENV=test npm run db:populateAllTables",
    "test": "node --experimental-vm-modules -r dotenv/config node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6"
  }
}
